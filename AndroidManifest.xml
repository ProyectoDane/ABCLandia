<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frba.abclandia"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".StartActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".MaestroListActivity"
            android:label="@string/title_activity_maestro_list"
            android:parentActivityName="android.app.LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="landscape"
                android:value="android.app.ListActivity" />
        </activity>
        <activity
            android:name=".AlumnoListActivity"
            android:label="@string/title_activity_maestro_list"
            android:parentActivityName="android.app.MaestroListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="landscape"
                android:value="android.app.ListActivity" />
        </activity>
        <activity
            android:name=".ActividadesActivity"
            android:label="@string/actividadesTitulo"
            android:parentActivityName="android.app.LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="landscape"
                android:value="android.app.ActividadesActivity" />
        </activity>
        <activity
            android:name=".JugarActivity"
            android:label="@string/title_activity_jugar" >
        </activity>
        <activity
            android:name="com.example.abclandia.MainActivity"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>